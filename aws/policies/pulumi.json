{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ManageStaticBucket",
      "Effect": "Allow",
      "Action": [
        "s3:CreateBucket",
        "s3:Get*",
        "s3:List*",
        "s3:PutBucketPublicAccessBlock",
        "s3:PutBucketPolicy",
        "s3:DeleteBucketPolicy"
      ],
      "Resource": [
        "arn:aws:s3:::static.pedaki.fr"
      ]
    },
    {
      "Sid": "ManageUsers",
      "Effect": "Allow",
      "Action": [
        "iam:CreateAccessKey",
        "iam:CreateUser"
      ],
      "Resource": [
        "*"
      ]
    }
  ]
}